{
  "name": "subly-extension",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:ext": "tsc -b && vite build --config vite.extension.config.ts",
    "package:ext": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/package-extension.ps1",
    "preview": "vite preview",
    "test": "vitest --run"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "gsap": "^3.14.2",
    "html2canvas": "^1.4.1",
    "motion": "^12.26.2",
    "ogl": "^1.0.11",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.270",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "3.4.14",
    "typescript": "^5.5.4",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
